{
  "name": "web-full-stack-boilerplate",
  "version": "1.0.0",
  "description": "The Boilerplate I created to build web app, with support to ReactJS, NodeJS, Express, Mongodb, mySQL, socket.io, webpack, hot-reload, etc.",
  "main": "index.js",
  "scripts": {
    "start": "node private/server.js",
    "prebuild": "rimraf public && rimraf private",
    "build:server": "webpack --config webpackConfig/webpack.server.config.js",
    "build:pageA": "webpack --config webpackConfig/webpack.prod.pageA.config.js",
    "build:pageB": "webpack --config webpackConfig/webpack.prod.pageB.config.js",
    "build:pageC": "webpack --config webpackConfig/webpack.prod.pageC.config.js",
    "build:pageIO": "webpack --config webpackConfig/webpack.prod.pageIO.config.js",
    "build": "npm-run-all build:*",
    "dev": "NODE_ENV=dev webpack-dashboard -- nodemon --exec babel-node server/server.js --ignore public/ --ignore client/",
    "win-dev": "set NODE_ENV=dev && webpack-dashboard -- nodemon --exec babel-node server/server.js --ignore public/ --ignore client/"
  },
  "author": "Scya597 a.k.a. Samuel Chan",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "knex": "^0.14.2",
    "mongodb": "^3.0.4",
    "mongoose": "^5.0.9",
    "mysql": "^2.15.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-minify": "^0.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jsdoc": "^3.5.5",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "^1.0.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.20.0"
  }
}
